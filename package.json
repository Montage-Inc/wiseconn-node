{
  "name": "wiseconn-node",
  "version": "1.0.0",
  "description": "Node wrapper for the Wiseconn API",
  "main": "build/Wiseconn.js",
  "author": "slangley <silaslangley98@gmail.com>",
  "license": "MIT",
  "scripts": {
    "test": "NODE_ENV=test node_modules/.bin/mocha --require babel-polyfill --compilers js:babel-core/register 'spec/**/*.js'",
    "lint": "./node_modules/.bin/eslint ./lib/**/*",
    "build": "yarn test && yarn lint && yarn flow && ./node_modules/.bin/babel ./lib --out-dir build",
    "push": "yarn build && yarn publish"
  },
  "dependencies": {
    "es6-promise": "^4.0.5",
    "moment": "^2.17.1"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-eslint": "^7.1.1",
    "babel-polyfill": "^6.22.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "eslint": "^3.12.1",
    "mocha": "^3.2.0"
  }
}
